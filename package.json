{
    "name": "react-cards",
    "version": "0.1.0",
    "private": true,
    "homepage": "http://centrodph.github.io/react-cards",
    "dependencies": {
        "axios": "^0.18.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.7",
        "react-router": "^4.2.0",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "^4.2.2",
        "react-scripts": "1.1.1",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0"
    },
    "scripts": {
